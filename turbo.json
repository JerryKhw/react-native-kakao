{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "ci:android:build:new": {
      "inputs": [
        "!android/build",
        "packages/**/src",
        "packages/**/android",
        "packages/core/expo-config-plugin",
        "example/android",
        "!example/android/.gradle",
        "!example/android/build",
        "!example/android/app/build",
        "!packages/**/lib",
        "!packages/**/android/build",
        "**/package.json"
      ],
      "outputs": []
    },
    "ci:android:build:old": {
      "inputs": [
        "!android/build",
        "packages/**/src",
        "packages/**/android",
        "packages/core/expo-config-plugin",
        "example/android",
        "!example/android/.gradle",
        "!example/android/build",
        "!example/android/app/build",
        "!packages/**/lib",
        "!packages/**/android/build",
        "**/package.json"
      ],
      "outputs": []
    },
    "ci:ios:build:new": {
      "inputs": [
        "packages/**/*.podspec",
        "packages/**/src",
        "packages/**/ios",
        "packages/core/expo-config-plugin",
        "example/ios",
        "!example/android/.gradle",
        "!example/android/build",
        "!example/android/app/build",
        "!example/ios/build",
        "!example/ios/Pods",
        "!packages/**/lib",
        "**/package.json"
      ],
      "outputs": []
    },
    "ci:ios:build:old": {
      "inputs": [
        "packages/**/*.podspec",
        "packages/**/src",
        "packages/**/ios",
        "packages/core/expo-config-plugin",
        "example/ios",
        "!example/android/.gradle",
        "!example/android/build",
        "!example/android/app/build",
        "!example/ios/build",
        "!example/ios/Pods",
        "!packages/**/lib",
        "**/package.json"
      ],
      "outputs": []
    }
  }
}
